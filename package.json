{
  "name": "devportal",
  "version": "0.18.0",
  "private": true,
  "scripts": {
    "predev": "node src/utils/get-rapidoc.mjs",
    "dev": "next dev",
    "prebuild": "node src/utils/get-rapidoc.mjs",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{ts,js,tsx,jsx,json}\"",
    "lint": "next lint",
    "postinstall": "husky install",
    "cz-commit": "cz",
    "release": "standard-version"
  },
  "dependencies": {
    "@vtex/brand-ui": "^0.46.0",
    "next": "^12.1.0",
    "node-fetch": "^3.2.4",
    "rapidoc": "^9.2.0",
    "react": "17.0.2",
    "react-animate-height": "^3.0.4",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^9.2.2",
    "react-markdown": "^8.0.3",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vtex/prettier-config": "^1.0.0",
    "@vtex/tsconfig": "^0.6.0",
    "cypress": "^9.5.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-vtex-react": "^8.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "i": "^0.3.7",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "rapidoc": {
      "version": "1.0.4-vtex"
    }
  }
}
